CREATE TABLE makanan (
 id_jenis_makanan INT PRIMARY KEY AUTO_INCREMENT,
 nama_makanan VARCHAR(255) NOT NULL,
 harga INT NOT NULL,
 jumlah INT NOT NULL
);

